<h1>{{title}}</h1>
<hr />
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<link href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
<table>
    
    <thead>
        
        <tr>
            <th>Date</th>
            <th>Start Time</th>
            <th>End Time</th>
            <th>
                <a class="btn btn-success" id="new" href="public/Exam/Final/home/edit">Add Time Slot</a>
            </th>
        </tr>
    </thead>
    <tbody>
{{#each quotes}}
        <tr>
            <td>{{date}}</td>
            <td>{{start_time}}</td>
            <td>{{end_time}}</td>
            <td>
                <a class="btn btn-primary" href="/Exam/Final/home/edit?id={{date}}">Edit</a>
            </td>
        </tr>
{{/each}}
    </tbody>
</table>
